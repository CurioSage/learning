#include<stdio.h>
#include<stdlib.h>
int read(int *ptr,int n)
{
    
    for(int i=0;i<n;i++)
    {
        scanf("%d", (ptr+i));
    }
    return 0;
}

int display(int *ptr,int n)
{
    
    for(int i=0;i<n;i++)
    {
        printf("%d\n",*(ptr+i));
    }
    return 0;
}

int main()
{
    int n,*ptr;
    scanf("%d",&n);
    int a[n];
    ptr = (int *)calloc(n,sizeof(int));
    read(ptr,n);
    display(ptr,n);
    free(ptr);

}



